# To use CycloneDDS
sudo apt install ros-jazzy-rmw-cyclonedds-cpp && export RMW_IMPLEMENTATION=rmw_cyclonedds_cpp

# From here: https://docs.ros.org/en/jazzy/Tutorials/Advanced/FastDDS-Configuration.html
export FASTRTPS_DEFAULT_PROFILES_FILE=/home/ubuntu/Desktop/ROS2/third_party/fast_dds_profile.xml

